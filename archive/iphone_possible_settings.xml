<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.medimage" version="1.3.21" versionCode="376" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MedImage</name>
    <description>
        Copy a camera image to your PC.
    </description>
    <author email="webmaster@atomjump.com" href="http://atomjump.com">
        AtomJump
    </author>
    
   
    <gap:plugin name="org.apache.cordova.file-transfer" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-networkinterface" source="npm" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.3.1" />
    <plugin name="cordova-plugin-globalization" spec="1.0.5" />
    <plugin name="cordova-plugin-camera" source="npm" spec="2.3.0">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="AtomJump MedImage would like to access your camera, so that you can take patient photos." />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="AtomJump MedImage would like to access your camera, so that you can take patient photos." />
    </plugin>
    <platform name="android">
        <preference name="permissions" value="none" />
        <preference name="orientation" value="portrait" />
        <preference name="fullscreen" value="false" />
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-feature android:name="android.hardware.camera" />
        <uses-feature android:name="android.hardware.camera.autofocus" />
        <icon height="128" src="icon.png" width="128" />
    </platform>
    <platform name="ios">
        <preference name="deployment-target" value="10.0" />
        <!--<preference name="phonegap-version" value="cli-6.4.0" />-->
        <preference name="BackupWebStorage" value="none" />
        <preference name="permissions" value="none" />
        <preference name="orientation" value="portrait" />
       
        <gap:config-file mode="add" parent="ITSAppUsesNonExemptEncryption" platform="ios">
            <false />
        </gap:config-file>
        <icon height="128" src="icon.png" width="128" />
        <icon height="180" platform="ios" src="icon-60@3x.png" width="180" />
        <icon height="57" platform="ios" src="icon-57.png" width="57" />
        <icon height="114" platform="ios" src="icon-57@2x.png" width="114" />
        <icon height="60" platform="ios" src="icon-60.png" width="60" />
        <icon height="120" platform="ios" src="icon-60@2x.png" width="120" />
        <icon height="76" platform="ios" src="icon-76.png" width="76" />
        <icon height="152" platform="ios" src="icon-76@2x.png" width="152" />
        <icon height="29" platform="ios" src="icon-small.png" width="29" />
        <icon height="58" platform="ios" src="icon-small@2x.png" width="58" />
        <icon height="40" platform="ios" src="icon-40.png" width="40" />
        <icon height="80" platform="ios" src="icon-40@2x.png" width="80" />
        <icon height="57" platform="ios" src="icon.png" width="57" />
        <icon height="114" platform="ios" src="icon@2x.png" width="114" />
        <icon height="72" platform="ios" src="icon-72.png" width="72" />
        <icon height="144" platform="ios" src="icon-72@2x.png" width="144" />
        <icon height="29" platform="ios" src="icon-small.png" width="29" />
        <icon height="58" platform="ios" src="icon-small@2x.png" width="58" />
        <icon height="50" platform="ios" src="icon-50.png" width="50" />
        <icon height="100" platform="ios" src="icon-50@2x.png" width="100" />
        <splash height="1242" platform="ios" src="img/Default-1242@3x~iphone6s-landscape_2208x1242.png" width="2208" />
        <splash height="2208" platform="ios" src="img/Default-1242@3x~iphone6s-portrait_1242x2208.png" width="1242" />
        <splash height="1136" platform="ios" src="img/Default-568h@2x~iphone_640x1136.png" width="640" />
        <splash height="750" platform="ios" src="img/Default-750@2x~iphone6-landscape_1334x750.png" width="1334" />
        <splash height="1334" platform="ios" src="img/Default-750@2x~iphone6-portrait_750x1334.png" width="750" />
        <splash height="1496" platform="ios" src="img/Default-Landscape@2x~ipad_2048x1496.png" width="2048" />
        <splash height="1536" platform="ios" src="img/Default-Landscape@2x~ipad_2048x1536.png" width="2048" />
        <splash height="748" platform="ios" src="img/Default-Landscape~ipad_1024x748.png" width="1024" />
        <splash height="768" platform="ios" src="img/Default-Landscape~ipad_1024x768.png" width="1024" />
        <splash height="2008" platform="ios" src="img/Default-Portrait@2x~ipad_1536x2008.png" width="1536" />
        <splash height="2048" platform="ios" src="img/Default-Portrait@2x~ipad_1536x2048.png" width="1536" />
        <splash height="1024" platform="ios" src="img/Default-Portrait~ipad_768x1024.png" width="768" />
        <splash height="1004" platform="ios" src="img/Default.png" width="768" />
        <splash height="960" platform="ios" src="img/Default@2x~iphone_640x960.png" width="640" />
        <splash height="2008" platform="ios" src="img/Default~ipad.png" width="1536" />
        <splash height="480" platform="ios" src="img/Default~iphone.png" width="320" />
    </platform>
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <!--<engine name="ios" spec="~4.1.1" />-->
</widget>
